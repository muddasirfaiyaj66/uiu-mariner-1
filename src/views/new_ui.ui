   <string notr="true">QMainWindow {
    background-color: #000000;
}

QWidget {
    color: #e0e0e0;
    font-family: &quot;Segoe UI&quot;, Arial, sans-serif;
    font-size: 10pt;
    background-color: #000000;
}

/* Sidebar Styles */
QFrame#side_frame {
    background-color: #0a0a0a;
    border-right: 1px solid #1a1a1a;
}

QFrame#logo_frame {
    background-color: #000000;
    border-bottom: 1px solid #1a1a1a;
}

/* Navigation Buttons */
QPushButton#btnDashboard, 
QPushButton#btnGallery, 
QPushButton#btnDataAnalysis, 
QPushButton#btnSettings {
    background-color: transparent;
    color: #cccccc;
    border: none;
    border-left: 3px solid transparent;
    padding: 12px 20px;
    text-align: left;
    font-size: 11pt;
}

QPushButton#btnDashboard:hover, 
QPushButton#btnGallery:hover, 
QPushButton#btnDataAnalysis:hover, 
QPushButton#btnSettings:hover {
    background-color: #1a1a1a;
    color: #ffffff;
}

QPushButton#btnDashboard:checked, 
QPushButton#btnGallery:checked, 
QPushButton#btnDataAnalysis:checked, 
QPushButton#btnSettings:checked {
    background-color: #1a1a1a;
    color: #ea8a35;
    border-left: 3px solid #ea8a35;
    font-weight: bold;
}

/* Sidebar Toggle Button */
QPushButton#btnToggleSidebar {
    background-color: transparent;
    border: none;
    color: #ea8a35;
    padding: 8px;
}

QPushButton#btnToggleSidebar:hover {
    background-color: #1a1a1a;
}

/* Main Frame */
QFrame#main_frame {
    background-color: #000000;
}

/* Top Bar */
QFrame#top_bar_frame {
    background-color: #0a0a0a;
    border-bottom: 1px solid #1a1a1a;
    padding: 4px;
}

/* Camera Frames */
QFrame#main_cam_frame, QFrame#cam2_frame {
    background-color: #0a0a0a;
    border: 1px solid #1a1a1a;
    border-radius: 8px;
    padding: 4px;
}

/* Sensor Panels */
QFrame#attitude_frame, QFrame#sensor_tab_frame, QFrame#control_frame {
    background-color: #0a0a0a;
    border: 1px solid #1a1a1a;
    border-radius: 6px;
    padding: 8px;
}

/* Data Display Frames */
QFrame#frame_pressure, QFrame#frame_speed, QFrame#frame_pixhawk,
QFrame#frame_thrusters, QFrame#frame_arm, QFrame#frame_joystick,
QFrame#frame_pitch, QFrame#frame_roll, QFrame#frame_battery,
QFrame#frame_temperature, QFrame#frame_comms, QFrame#frame_camera_status,
QFrame#frame_lights, QFrame#frame_depth {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #0f0f0f, stop:1 #0a0a0a);
    border: 1px solid #1a1a1a;
    border-left: 3px solid #00d4ff;
    border-radius: 6px;
    padding: 10px;
    margin: 2px;
}

QFrame#frame_pressure:hover, QFrame#frame_speed:hover, QFrame#frame_pixhawk:hover,
QFrame#frame_thrusters:hover, QFrame#frame_arm:hover, QFrame#frame_joystick:hover,
QFrame#frame_pitch:hover, QFrame#frame_roll:hover, QFrame#frame_battery:hover,
QFrame#frame_temperature:hover, QFrame#frame_comms:hover, QFrame#frame_camera_status:hover,
QFrame#frame_lights:hover, QFrame#frame_depth:hover {
    border-left: 3px solid #ea8a35;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #151515, stop:1 #0f0f0f);
}

/* Gallery Grid Items */
QFrame#gallery_item_frame_1, QFrame#gallery_item_frame_2,
QFrame#gallery_item_frame_3, QFrame#gallery_item_frame_4 {
    background-color: #0a0a0a;
    border: 1px solid #1a1a1a;
    border-radius: 8px;
    padding: 8px;
}

QFrame#gallery_item_frame_1:hover, QFrame#gallery_item_frame_2:hover,
QFrame#gallery_item_frame_3:hover, QFrame#gallery_item_frame_4:hover {
    border: 1px solid #ea8a35;
}

/* Settings Sections */
QFrame#settings_section_frame_camera,
QFrame#settings_section_frame_network,
QFrame#settings_section_frame_system {
    background-color: #0a0a0a;
    border: 1px solid #1a1a1a;
    border-radius: 8px;
    padding: 16px;
}

/* Data Analysis Charts */
QFrame#chart_frame_depth, QFrame#chart_frame_temperature,
QFrame#chart_frame_pressure, QFrame#chart_frame_battery {
    background-color: #0a0a0a;
    border: 1px solid #1a1a1a;
    border-radius: 8px;
    padding: 8px;
}

/* Labels - Futuristic Styling */
QLabel {
    color: #00d4ff;
    background-color: transparent;
    font-family: 'Segoe UI', 'Roboto', sans-serif;
}

QLabel#lblTitle, QLabel#lblCam1, QLabel#lblCam2,
QLabel#lblAttitudeTitle, QLabel#lblSensorDataTitle, QLabel#lblControlTitle {
    color: #00ffff;
    font-weight: bold;
    font-size: 11pt;
    letter-spacing: 1px;
    text-transform: uppercase;
}

/* Control Buttons */
QPushButton#btnLightToggle, QPushButton#btnArmThrusters {
    background-color: #0e639c;
    color: #ffffff;
    border: none;
    padding: 10px 16px;
    border-radius: 4px;
    font-weight: bold;
}

QPushButton#btnLightToggle:hover, QPushButton#btnArmThrusters:hover {
    background-color: #1177bb;
}

QPushButton#btnEmergencyStop {
    background-color: #c42b1c;
    color: #ffffff;
    border: none;
    padding: 10px 16px;
    border-radius: 4px;
    font-weight: bold;
    font-size: 11pt;
}

QPushButton#btnEmergencyStop:hover {
    background-color: #e03e2e;
}

QPushButton#btnArmConnect {
    background-color: #107c10;
    color: #ffffff;
    border: none;
    padding: 8px 16px;
    border-radius: 4px;
    font-weight: bold;
}

QPushButton#btnArmConnect:hover {
    background-color: #0f8c0f;
}

/* Status Indicators */
QLabel#lblStatus {
    padding: 6px 12px;
    border-radius: 4px;
    font-weight: bold;
}

/* Tab Widget */
QTabWidget::pane {
    border: 1px solid #1a1a1a;
    background-color: #0a0a0a;
    border-radius: 4px;
    padding: 4px;
}

QTabBar::tab {
    background-color: #0f0f0f;
    color: #cccccc;
    padding: 8px 16px;
    border: 1px solid #1a1a1a;
    border-bottom: none;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}

QTabBar::tab:selected {
    background-color: #0a0a0a;
    color: #ea8a35;
    font-weight: bold;
    border: 1px solid #1a1a1a;
    border-bottom: none;
}

QTabBar::tab:hover {
    background-color: #2a2a2a;
    color: #ffffff;
}

/* Scroll Bars */
QScrollBar:vertical {
    border: none;
    background: #0a0a0a;
    width: 10px;
    border-radius: 5px;
}

QScrollBar::handle:vertical {
    background: #3a3a3a;
    border-radius: 5px;
    min-height: 20px;
}

QScrollBar::handle:vertical:hover {
    background: #ea8a35;
}

QScrollBar:horizontal {
    border: none;
    background: #0a0a0a;
    height: 10px;
    border-radius: 5px;
}

QScrollBar::handle:horizontal {
    background: #3a3a3a;
    border-radius: 5px;
    min-width: 20px;
}

QScrollBar::handle:horizontal:hover {
    background: #ea8a35;
}

/* Line Edits and Combo Boxes */
QLineEdit, QComboBox {
    background-color: #0f0f0f;
    border: 1px solid #1a1a1a;
    border-radius: 4px;
    padding: 8px;
    color: #ffffff;
}

QLineEdit:focus, QComboBox:focus {
    border: 1px solid #ea8a35;
    background-color: #1a1a1a;
}

QComboBox::drop-down {
    border: none;
    background: #2a2a2a;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
}

QComboBox::down-arrow {
    image: none;
    border-left: 5px solid transparent;
    border-right: 5px solid transparent;
    border-top: 5px solid #000000;
}</string>
